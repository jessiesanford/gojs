;\n}\n\na:link, a:visited {\n  color: #dc3647;\n  text-decoration: none;\n}\n\na:hover {\n  cursor: pointer;\n}\n\n.wrapper {\n  width: 80%;\n  margin: auto;\n}\n\n.crisp-wrapper {\n  //display: inline-block;\n  position: relative;\n  width: 100%;\n  height: 100%;\n}\n\n.crisp-sidebar {\n  width: 300px;\n}\n\n.crisp-nav {\n  display: flex;\n  align-items: center;\n  background: #000;\n  height: 50px;\n  padding: 0 30px;\n  color:#a0a0a0;\n\n  .sidebar-toggle {\n    margin-right: 20px;\n  }\n\n  .main-logo {\n    color: #fff;\n    font-size: 24px;\n    font-weight: bold;\n  }\n}\n\n.inner-wrapper {\n  position: relative;\n  display: flex;\n  flex-grow: 1;\n  width: 100%;\n  height: 100%;\n}\n\n.cytoscape-container {\n  width: 100%;\n  height: 100%;\n  background: #333738;\n}\n\n.flex-box {\n  display: flex;\n\n  &.flex-box--center {\n    align-items: center;\n  }\n}\n\n.flex-cell-right {\n  margin-left: auto;\n}\n\n.writing-pane {\n  background: #000;\n  width: 140px;\n\n  .cxtmenu-options-list {\n    .cxtmenu-option {\n      color: #fff;\n      padding: 5px 10px;\n      border-bottom: 1px solid #606060;\n\n      &:hover {\n        background: #202020;\n        cursor: pointer;\n      }\n\n      &:last-child {\n        border-bottom: 0;\n      }\n    }\n  }\n}\n\n.main-header {\n  background: #202020;\n  padding: 20px 0;\n\n  .logo {\n    img {\n      max-height: 30px;\n    }\n  }\n\n  ul.header-nav {\n    margin: 0;\n    margin-left: auto;\n    padding: 0;\n    list-style: none;\n    display: flex;\n    align-items: center;\n\n    .header-nav__item {\n      padding: 10px;\n      a:link, a:visited {\n        color: #fff;\n        font-weight: bold;\n      }\n      a:hover {\n        color: #dc3647;\n      }\n    }\n  }\n}\n\n@z-mask: 8;\n@z-modal: 9;\n\n.modal-wrapper {\n\n  .modal-mask {\n    position: fixed;\n    background: rgba(0,0,0,0.5);\n    width: 100%;\n    height: 100%;\n    top: 0;\n    left: 0;\n    z-index: @z-mask;\n  }\n\n  .modal-container {\n    background: #323537;\n    width: 500px;\n    position: absolute;\n    left: 0;\n    right: 0;\n    margin: 0 auto 0 auto;\n    color: #a0a0a0;\n    z-index: @z-modal;\n    box-shadow: 0 0 30px rgba(0,0,0,0.65);\n    border-radius: 2px;\n\n    .modal-container__heading {\n      display: flex;\n      align-items: center;\n      height: 30px;\n      padding: 10px;\n      font-size: 14px;\n      text-transform: uppercase;\n      color: #fff;\n    }\n\n    .modal-container__body {\n      padding: 10px;\n    }\n\n    .modal-container__controls {\n      display: flex;\n      align-items: center;\n      justify-content: flex-end;\n      padding: 10px;\n    }\n\n    .modal-close {\n      position: absolute;\n      top: 15px;\n      right: 10px;\n      font-size: 18px;\n      cursor: pointer;\n    }\n  }\n}\n\n.btn {\n  background: #439dea;\n  border: 0;\n  border-radius: 2px;\n  padding: 5px 10px;\n  color: #fff;\n  font: 14px \"Roboto\", Helvetica, Arial, sans-serif;\n  cursor: pointer;\n  text-transform: uppercase;\n\n  &.btn--alt {\n    background: #808080;\n  }\n}\n\n.push-right-5 {\n  margin-right: 5px;\n}\n\n.push-right-10 {\n  margin-right: 10px;\n}"},"lineCount":null}},"error":null,"hash":"f470a19ddc8d2fdd08fd69f8c5d078da","cacheData":{"env":{}}}